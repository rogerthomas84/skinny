{
    "name": "rogerthomas84/skinny",
    "description": "Skinny Library",
    "keywords": ["skinny"],
    "homepage": "https://github.com/rogerthomas84/skinny",
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Thomas",
            "email": "rogere84@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-iconv": "*",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-ctype": "*"
    },
    "suggest": {
        "ext-memcache": "*"
    },
    "autoload": {
        "psr-4": { "Skinny\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "SkinnyTests\\": "tests/" }
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    }
}
